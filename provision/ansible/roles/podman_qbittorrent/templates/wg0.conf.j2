{{ ansible_managed|comment }}

[Interface]
PrivateKey = {{ vpn_privatekey }}
Address = {{ vpn_address }}/32
DNS = {{ vpn_dns }}

PostUp = ip route add {{ cidr_local }} via {{ podman_proton_network_gateway }} dev eth0

[Peer]
PublicKey = {{ vpn_publickey }}
AllowedIPs = 0.0.0.0/0
Endpoint = {{ vpn_endpoint_ip }}:{{ vpn_endpoint_port}}
